from cmath import *

def racines_niemes(z, n):
    r = abs(z)
    theta = phase(z)
    
    r_n = r**(1/n)
    
    racines = []
    
    for k in range(n):
        angle = (theta + 2 * pi * k) / n
        racine = r_n * (cos(angle) + sin(angle) * 1j)
        racines.append(racine)
    
    return racines

z = 1 + 1j  
n = 3  
racines = racines_niemes(z, n)

for i, racine in enumerate(racines):
    print(f"Racine {i+1}: {racine}")
